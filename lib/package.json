{
  "name": "ng2-pdfjs-viewer",
  "version": "20.4.0",
  "author": {
    "name": "Aneesh Goapalakrishnan",
    "email": "codehippie1@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/intbot/ng2-pdfjs-viewer"
  },
  "publishConfig": {
    "access": "public"
  },
  "module": "dist/fesm2022/ng2-pdfjs-viewer.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/fesm2022/ng2-pdfjs-viewer.mjs"
    }
  },
  "devDependencies": {
    "ng-packagr": "^20.0.0",
    "shx": "^0.3.4"
  },
  "peerDependencies": {
    "@angular/common": "^20.0.0",
    "@angular/core": "^20.0.0",
    "ng-packagr": "^20.0.0"
  },
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "bugs": {
    "url": "https://github.com/intbot/ng2-pdfjs-viewer/issues"
  },
  "keywords": [
    "angular",
    "ng",
    "ng2",
    "pdfjs",
    "viewerjs",
    "viewer.html"
  ],
  "license": "Apache",
  "scripts": {
    "clean": "shx rm -rf ./dist",
    "build": "npm run clean && ng-packagr -p ng-package.json",
    "package": "npm run build && cd dist && npm pack",
    "bump": "mkdir .git && npm version patch",
    "version": "npm run build && git add -A .",
    "postversion": "git push && git push --tags && rmdir .git"
  }
}
